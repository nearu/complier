.data
string0: .asciiz "value of a: "
.text
add $fp,$sp,$zero
addi $sp,$sp,-4
addi $t0,$zero,10
sw $t0, 0($fp)
loop0:
lw $t2, 0($fp)
add $t1,$zero,$zero
addi $t3,$zero,20
beq $t2,$t3,equal0
addi $t1,$t1,-1
equal0:
addi $t1,$t1,1
bne $t1,$zero,break0
la $t2 string0
add $a0,$t2,$zero
addi $v0, $zero, 4
syscall
addi $t0,$t0,1
j loop0
break0:
addi $sp,$sp,4
j exit
exit:
